{
  "name": "link-list-server",
  "version": "0.0.1",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.12.3",
    "lodash": "^4.12.0",
    "nconf": "^0.10.0",
    "q": "^1.3.0",
    "request": "^2.87.0",
    "babel-preset-react-app": "^3.1.1",
    "joi-browser": "^13.0.1",
    "simple-git": "^1.91.0",
    "ws": "^0.7.1"
  },
  "devDependencies": {
    "jest": "20.0.4",
    "babel-jest": "23.0.1"
  },
  "engines": {
    "node": "9.5.0"
  },
  "scripts": {
    "start": "node server/src/server.js",
    "test": "node server/scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/server/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/server/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/server/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/server/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
