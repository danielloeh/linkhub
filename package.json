{
  "name": "link-list-server",
  "version": "0.0.1",
  "dependencies": {
    "auth0-js": "^9.10.1",
    "babel-preset-react-app": "^3.1.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "joi-browser": "^13.0.1",
    "jwks-rsa": "^1.4.0",
    "lodash": "^4.17.11",
    "nconf": "^0.10.0",
    "q": "^1.3.0",
    "request": "^2.87.0",
    "simple-git": "^1.91.0",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0"
  },
  "engines": {
    "node": "9.5.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./server/",
    "start": "node server/src/server.js",
    "test": "node server/scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/server/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/server/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/server/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/server/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}